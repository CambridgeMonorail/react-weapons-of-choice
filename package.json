  "description": "An Nx Monorepo setup with React, Vite, Tailwind CSS, and Shadcn UI.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CambridgeMonorail/react-weapons-of-choice.git"
  },
  "keywords": [
    "nx",
    "react",
    "tailwindcss",
    "shadcn-ui",
    "monorepo",
    "vite"
  ],
  "author": "Your Name <your-email@example.com>",
  "bugs": {
    "url": "https://github.com/CambridgeMonorail/react-weapons-of-choice/issues"
  },
  "scripts": {
    "build:affected": "nx affected --target=build",
    "build:all": "nx run-many --target=build --all",
    "build": "nx build",
    "clean": "nx run-many --target=clean --all",
    "format:check": "nx format:check",
    "format": "nx format:write",
    "lint:affected": "nx affected --target=lint",
    "lint:all": "nx run-many --target=lint --all",
    "lint": "nx lint",
    "precommit": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build",
    "prepare": "husky",
    "serve:demo": "nx serve demo",
    "test:affected": "nx affected --target=test",
    "test:all": "nx run-many --target=test --all",
    "test:e2e": "nx e2e",
    "test": "nx test",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm run precommit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "nx lint",
      "nx test --bail --findRelatedTests",
      "nx build --dry-run"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
